{
  "root": true,
  "plugins": [
    "jest"
  ],
  "extends": [
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jest/recommended",
    "plugin:sonarjs/recommended",
    "xo/esnext",
    "prettier"
  ],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "new-cap": [
      "error",
      {
        "properties": true
      }
    ],
    "no-console": "error"
  }
}
